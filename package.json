{
  "name": "svg-path-command-parser",
  "version": "0.0.1",
  "files": [
    "/dist",
    "/types"
  ],
  "main": "./dist/svg-path-command-parser.umd.js",
  "module": "./dist/svg-path-command-parser.es.js",
  "exports": {
    ".": {
      "import": "./dist/svg-path-command-parser.es.js",
      "require": "./dist/svg-path-command-parser.umd.js"
    }
  },
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "tsc && vite build",
    "watch": "vite build --watch",
    "test": "jest",
    "typeCheck": "tsc --noEmit",
    "lint:js": "eslint . --ext .js,.ts --ignore-path .gitignore",
    "lint:js:fix": "eslint . --ext .js,.ts --ignore-path .gitignore",
    "lint:code": "prettier . --ignore-path .gitignore",
    "lint:code:fix": "prettier --write . --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:code",
    "lint:fix": "npm run lint:js:fix && npm run lint:code:fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  }
}
